{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s - %(levelname)s - %(name)s - %(filename)s - %(lineno)s - %(message)s"
        }
    },
    "handlers": {
        "streamHandler": {
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        },
        "nullHandler": {
            "class": "logging.NullHandler"
        }
    },
    "loggers": {
        "sgnlp": {
            "qualname": "sgnlp",
            "handlers": ["streamHandler", "nullHandler"],
            "level": "DEBUG",
            "propagate": false
        }
    }
}